{
  "name": "starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install": "npm --prefix client install",
    "start": "node ./bin/www",
    "start:development": "nodemon --ignore client/ -r dotenv/config ./bin/www",
    "start:production": "./bin/www",
    "db:create": "dotenv sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all",
    "render-postbuild": "npm  --prefix client run build",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client",
    "sequelize": "npm run sequelize",
    "sequelize-cli": "npm run sequelize-cli",
    "build": "node psql-setup-script.js"
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.13.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "express-async-handler": "^1.1.4",
    "express-validator": "^6.4.1",
    "helmet": "^3.22.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "per-env": "^1.0.2",
    "pg": "^8.3.0",
    "react-redux": "^7.2.1",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1",
    "typescript": "^2.9.2",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "nodemon": "^2.0.3"
  }
}
